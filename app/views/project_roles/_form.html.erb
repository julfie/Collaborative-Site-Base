<%= simple_form_for(@project_role) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :role_id %>
    <%= f.input :user_id %>
    <%= f.input :invite_num %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
